# 基础配置
HOSTADD=localhost:8080
GROUPID=123456789
TARGETID=987654321
CHARACTER = default

# AI配置
AI_KEY=your_ai_api_key
AI_BASEURL=https://api.openai.com/v1
AI_MODEL=gpt-3.5-turbo
AI_PROMPT=

# AI 可选配置
AI_TEMPERATURE=1
AI_MAX_TOKENS=2048
AI_TOP_P=0.9
AI_TIMEOUT=30

# 启用自然定时器 (true/false)
ENABLE_NATURAL_SCHEDULER=true

# 启用情感记忆 (true/false)
ENABLE_EMOTIONAL_MEMORY=true

# 启用长对话模式 (true/false)
ENABLE_ONLY_LONG_CHAT=true

# 活跃时间段 (24小时制，逗号分隔)
ACTIVE_HOURS=9,10,11,14,15,16,19,20,21

# 休息时间段 (24小时制，逗号分隔)
SLEEP_HOURS=0,1,2,3,4,5,6,7,8,22,23

# 基础发送间隔 (分钟)
BASE_INTERVAL=45

# 随机因子 (0.0-1.0，影响发送时间的随机性)
RANDOM_FACTOR=0.5